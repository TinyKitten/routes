{
  "db_name": "MySQL",
  "query": "(SELECT\n                s.*,\n                l.line_name, \n                (\n                  6371 * acos(\n                    cos(\n                      radians(s.lat)\n                    ) * cos(\n                      radians(?)\n                    ) * cos(\n                      radians(?) - radians(s.lon)\n                    ) + sin(\n                      radians(s.lat)\n                    ) * sin(\n                      radians(?)\n                    )\n                  )\n                ) AS distance\n              FROM `stations` AS s\n              JOIN `lines` AS l ON l.line_cd = s.line_cd AND l.e_status = 0\n              WHERE\n                s.station_cd = s.station_g_cd\n                AND s.e_status = 0\n              ORDER BY \n                distance\n              LIMIT 1)\n              UNION\n              (SELECT\n                s.*,\n                l.line_name, \n                (\n                  6371 * acos(\n                    cos(\n                      radians(s.lat)\n                    ) * cos(\n                      radians(?)\n                    ) * cos(\n                      radians(?) - radians(s.lon)\n                    ) + sin(\n                      radians(s.lat)\n                    ) * sin(\n                      radians(?)\n                    )\n                  )\n                ) AS distance\n              FROM `stations` AS s\n              JOIN `lines` AS l ON l.line_cd = s.line_cd AND l.e_status = 0\n              WHERE\n                s.station_cd = s.station_g_cd\n                AND s.e_status = 0\n              ORDER BY \n                distance\n              LIMIT 1)",
